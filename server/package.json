{
  "name": "prm-server",
  "version": "1.0.0",
  "description": " server",
  "main": "index.js",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon -e ts -w ./src --exec \"npm run start\"",
    "build": "tsc",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrate": "npm run typeorm migration:run",
    "seed": "npm run typeorm -- -c seed migration:run",
    "seed:create": "npm run typeorm -- -c seed migration:generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danastasija/PRM.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danastasija/PRM/issues"
  },
  "homepage": "https://github.com/danastasija/PRM#readme",
  "dependencies": {
    "@types/mime": "^2.0.2",
    "argon2": "^0.26.2",
    "body-parser": "^1.18.1",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-jwt": "^5.3.3",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.4.6",
    "multer": "^1.4.2",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.24"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.3",
    "@types/node": "^8.10.61",
    "nodemon": "^2.0.4",
    "ts-node": "3.3.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  }
}
